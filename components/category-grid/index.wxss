.container {
    width: 100%;
    height: 320rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.l-inner-container {
    width: 730rpx;
    height: 300rpx;
    border: 1px dashed #DCEBE6;
}

.grid-item {
    width: 200rpx;
    height: 150rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.img {
    width: 60rpx;
    height: 60rpx;
}

.text {
    font-size: 30rpx;
    margin-top: 10rpx;
}
