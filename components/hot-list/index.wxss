@import "../../common/global.wxss";

.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 40rpx;
    background-color: #ffffff;
}

.title-img{
    width: 690rpx;
    height: 90rpx;
}

.inner-container{
    margin-top: 30rpx;
    display: flex;
    flex-direction: row;
    /*两边对齐 中间留白*/
    width: 100%;
    justify-content: space-between;
}

.right-container{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.left-img{
    display: flex;
    width: 346rpx;
    height: 522rpx;
}

.right-img{
    display: flex;
    width: 400rpx;
    height: 260rpx;
}
